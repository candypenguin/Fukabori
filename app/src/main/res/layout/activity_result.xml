<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Result">

    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline7"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.15"/>
    <android.support.constraint.Barrier android:layout_width="wrap_content" android:layout_height="wrap_content"
                                        android:id="@+id/barrier" app:barrierDirection="top"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline8"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.3"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline9"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.45"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline10"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.6"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline11"
                                          android:orientation="horizontal"
                                          app:layout_constraintGuide_percent="0.75"/>
    <TextView
            android:text="深掘り一覧"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/title" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/guideline7" android:textSize="24sp"
            android:textAlignment="center"/>
    <TextView
            android:text="何がしたいのか？　："
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView2" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline8"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline7"
            app:layout_constraintEnd_toStartOf="@+id/guideline13" android:layout_marginEnd="8dp"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:layout_width="190dp"
            android:layout_height="74dp"
            android:id="@+id/goal"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" tools:layout_editor_absoluteY="98dp"
            tools:layout_editor_absoluteX="214dp"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline13"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.5"/>
    <TextView
            android:text="なぜ："
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView4" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline13" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline8"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline9"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:layout_width="190dp"
            android:layout_height="74dp"
            android:id="@+id/firstwhy"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" tools:layout_editor_absoluteY="188dp"
            tools:layout_editor_absoluteX="214dp"/>
    <TextView
            android:text="なぜ："
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView6" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline13" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/textView4"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline10"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/secondwhy" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline9"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline10"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:text="なぜ："
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/textView8" app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintEnd_toStartOf="@+id/guideline13" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline11"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <TextView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/thirdwhy" app:layout_constraintStart_toStartOf="@+id/guideline13"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/guideline10"
            android:layout_marginBottom="8dp" app:layout_constraintBottom_toTopOf="@+id/guideline11"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"/>
    <Button
            android:text="完了"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/finishButton" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/guideline11" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent"/>
</android.support.constraint.ConstraintLayout>